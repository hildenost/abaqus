<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE language>
<!-- Abaqus input file syntax highlightning v0.01 by Hilde Aas Nøst -->
<language name="Abaqus" version="1" kateversion="5.0" section="Scientific" extensions="*.inp" casesensitive="0" author="Hilde Aas Nøst" license="">
<highlighting>
    <contexts>
        <context attribute="Normal Text" lineEndContext="#stay" name="Normal">
            <Detect2Chars context="Comment" char="*" char1="*" />
        </context>
        <context attribute="Comment" lineEndContext="#pop" name="Comment" />
    </contexts>
    <itemDatas>
        <itemData name="Normal" defStyleNum="dsNormal" />
        <itemData name="Comment" defStyleNum="dsComment" />
    </itemDatas>
</highlighting>
<general>
    <comments>
        <comment name="singleLine" start="**"/>
    </comments>
    <keywords casesensitive="0"/>
</general>
</language>

